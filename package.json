    "eslint": "^0.24.1",
    "eslint-plugin-react": "^2.7.0",
    "expect": "^1.8.0",
    "mocha": "^2.2.5",
    "mocha-babel": "^3.0.0",
    "node-jsdom": "^3.1.5",
